{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "object-curly-spacing": ["warn", "always"],
    "no-unused-vars": "warn",
    "require-jsdoc": "off",
    "max-len": [
      "warn",
      {
        "code": 100
      }
    ],
    "operator-linebreak": "off",
    "no-console": "warn",
    "new-cap": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
